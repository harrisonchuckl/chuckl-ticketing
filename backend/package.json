{
  "name": "chuckl-ticketing-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
"scripts": {
  "prebuild": "npm run prisma:generate",
  "build": "tsc -p tsconfig.json",
  "start": "node dist/start.js",
  "dev": "node --watch --loader ts-node/esm src/server.ts",
  "prisma:generate": "prisma generate --schema=backend/prisma/schema.prisma",
  "prisma:migrate": "prisma migrate deploy --schema=backend/prisma/schema.prisma"
}
  "dependencies": {
    "@aws-sdk/client-s3": "^3.929.0",
    "@aws-sdk/lib-storage": "^3.929.0",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "busboy": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "jose": "^5.10.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.11",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.15.2",
    "resend": "^6.6.0",
    "sharp": "^0.33.5",
    "stripe": "^16.12.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^7.0.4",
    "@types/pdfkit": "^0.13.6",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3"
  }
}
